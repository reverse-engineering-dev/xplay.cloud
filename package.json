{
  "name": "xplay",
  "version": "1.0.0",
  "description": "xplay web site",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node index.js",
    "server": "cross-env NODE_ENV=development  nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install:client": "npm install --prefix client",
    "build:client": "npm run build --prefix client"
  },
  "keywords": [
    "xbox",
    "xplay",
    "gaming",
    "web"
  ],
  "author": "cikpak",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "mqtt": "^4.2.4",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "rooty": "^2.0.0",
    "uniqid": "^5.2.0",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.5"
  },
  "devDependencies": {
    "faker": "^5.1.0"
  }
}
